double_buffer yes
alignment bottom_middle
background yes
border_width 1
cpu_avg_samples 2
default_color 222222
default_outline_color 222222
draw_borders no
border_margin 0
border_width 0
draw_graph_borders yes
draw_outline no
draw_shades no
use_xft yes
xftfont Sans Mono:size=10
gap_x 5
gap_y 0
minimum_size 1902
maximum_width 1920
net_avg_samples 2
no_buffers yes
out_to_console no
out_to_stderr no
extra_newline no
own_window no
own_window_class Conky
own_window_type desktop
own_window_transparent yes
stippled_borders 0
update_interval 1.0
uppercase no
use_spacer left
show_graph_scale no
show_graph_range no
format_human_readable yes

color1 666666
color2 888888
color3 444444

mpd_host 127.0.0.1

TEXT
${color1}Kernel: ${color}${kernel}  \
${color1}CPU: ${color}${execi 1 sensors | grep "CPU Temperature" | awk '{print $3}'}C  \
${color1}MB: ${color}${execi 1 sensors | grep "MB Temperature" | awk '{print $3}'}C  \
${color1}NB: ${color}${execi 1 sensors | grep "NB Temperature" | awk '{print $3}'}C  \
${color1}SB: ${color}${execi 1 sensors | grep "SB Temperature" | awk '{print $3}'}C  \
${color1}R1: ${color}${execi 1 sudo hddtemp /dev/disk/by-id/ata-WDC_WD20EARS-07MVWB0_WD-WCAZA8004341 | awk '{print $4}'}C  \
${color1}R2: ${color}${execi 1 sudo hddtemp /dev/disk/by-id/ata-WDC_WD20EARS-07MVWB0_WD-WCAZA8005607 | awk '{print $4}'}C  \
${color1}CPU: ${color} ${cpu cpu0}%  \
${goto 686}\
${color1}RAM: ${color} ${memperc}%  \
${color1}Root: ${color} ${fs_used_perc /}%  \
${color1}Home: ${color} ${fs_used_perc /home}%  \
${goto 900}\
${color1}Proc: ${color} ${goto 924}${running_threads} /${threads}  \
${goto 986}\
${color1}IO: ${color} ${top_io name 1}  \
${goto 1090}\
${color1}Load: ${color} ${loadavg}  \
${color1}Entropy: ${color} ${entropy_avail}  \
${goto 1500} ${color1}eth0 ${color}(${addr eth0}${color}): \
${goto 1620} ${color1}tx:${color} ${upspeed eth0} (${totalup eth0}) \
${goto 1760} ${color1}rx:${color} ${downspeed eth0} (${totaldown eth0}) \
